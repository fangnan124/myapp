<h1>Listing business_descriptions</h1>

<table>
  <thead>
    <tr>
      <th>Field</th>
      <th>Programming language</th>
      <th>Business</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @business_descriptions.each do |business_description| %>
      <tr>
        <td><%= business_description.field %></td>
        <td><%= business_description.programming_language %></td>
        <td><%= business_description.business %></td>
        <td><%= link_to 'Show', business_description %></td>
        <td><%= link_to 'Edit', edit_business_description_path(business_description) %></td>
        <td><%= link_to 'Destroy', business_description, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

