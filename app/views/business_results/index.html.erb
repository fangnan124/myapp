<h1>Listing business_results</h1>

<table>
  <thead>
  <tr>
    <th>System name</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @business_results.each do |business_result| %>
      <tr>
        <td><%= business_result.system_name %></td>
        <td><%= business_result.content %></td>
        <td><%= link_to 'Show', business_result %></td>
        <td><%= link_to 'Edit', edit_business_result_path(business_result) %></td>
        <td><%= link_to 'Destroy', business_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

